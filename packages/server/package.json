{
  "name": "@jobperday/server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node -r tsconfig-paths/register -r dotenv-safe/config src/index.ts",
    "start:dev": "NODE_ENV=development concurrently --handle-input \"wait-on dist/index.js && nodemon\" \"tsc -w -p tsconfig.build.json\"",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/index.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "NODE_ENV=test npm run test:e2e",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/graphql": "^6.4.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^6.1.3",
    "apollo-server-express": "^2.8.1",
    "async-exit-hook": "^2.0.1",
    "cls-hooked": "^4.2.2",
    "connect-redis": "^3.4.2",
    "dotenv-safe": "^6.1.0",
    "graphql": "^14.4.2",
    "graphql-tools": "^4.0.5",
    "ioredis": "^4.14.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.17.2",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "type-graphql": "^0.17.4",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/cls-hooked": "^4.3.0",
    "@types/connect-redis": "^0.0.10",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.5",
    "@types/ioredis": "^4.0.14",
    "@types/jest": "^23.3.13",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/luxon": "^1.15.2",
    "@types/node": "^10.12.18",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.5",
    "@types/winston": "^2.4.4",
    "concurrently": "^4.1.0",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  },
  "workspaces": {
    "nohoist": [
      "typeorm",
      "typeorm/**"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
